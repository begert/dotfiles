
# SUPress ERror FIND
function superfind() {
    echo "Errors will be suppressed"
    find . -name "$@" 2>/dev/null
}

# List oh-my-zsh git plugin aliases
function gitsc() {
    cat ~/.oh-my-zsh/plugins/git/git.plugin.zsh | grep alias | less
}

function formatjson() {
    python -m json.tool $1 > $1.formatted.json
    mv $1.formatted.json $1
}
